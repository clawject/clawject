{
  "name": "clawject",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "nx run-many --target=lint",
    "test": "yarn run clear-yarn-clawject-cache && node tests/runTests.js",
    "build": "nx run-many --target=build",
    "registry:start": "ts-node scripts/registry",
    "registry:stop": "ts-node scripts/registry/stop",
    "release": "nx release --skip-publish",
    "publish:local": "nx release publish --registry http://localhost:4873",
    "clear-yarn-clawject-cache": "rm $(yarn config get cacheFolder)/@clawject* || true",
    "local:test": "yarn run build && yarn run registry:start && yarn run publish:local && yarn run test"
  },
  "private": true,
  "devDependencies": {
    "@microsoft/api-extractor": "^7.43.7",
    "@nx/eslint": "19.0.2",
    "@nx/eslint-plugin": "19.0.2",
    "@nx/jest": "19.0.2",
    "@nx/js": "19.0.2",
    "@nx/plugin": "19.0.2",
    "@nx/workspace": "19.0.2",
    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "command-line-args": "^5.2.1",
    "copyfiles": "^2.4.1",
    "exec-sh": "^0.4.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "19.0.2",
    "pm2": "^5.3.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "verdaccio": "^5.31.0"
  },
  "dependencies": {
    "@nx/devkit": "19.0.2",
    "tslib": "^2.3.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "nx": {
    "includedScripts": []
  },
  "packageManager": "yarn@4.4.0"
}
